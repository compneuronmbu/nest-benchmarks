Compile Megware CPU 8160
# get https://gitlab.com/astron-idg/idg
module load comp/cmake/3.7.2
mkdir build; cd build;
# do cmake and then ccmake . and ON MKL, other fttw3 error
cmake -DCMAKE_INSTALL_PREFIX=~/tmp/ ..
make 
# launch executable
cd ~/tmp/bin/examples/cxx/
export KMP_AFFINITY=granularity=fine,compact,1,0  
export OMP_NUM_THREADS=48
./cpu-optimized.x

